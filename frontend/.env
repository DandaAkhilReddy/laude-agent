# Enterprise Laude Agent - Frontend Configuration
# HSS Medicine React Application

# Backend API Configuration
REACT_APP_BACKEND_URL=http://localhost:8001

# Application Configuration
REACT_APP_NAME=Laude Agent Enterprise
REACT_APP_VERSION=2.0.0
REACT_APP_COMPANY=HSS Medicine
REACT_APP_DOMAIN=hssmedicine.com

# Environment
REACT_APP_ENV=development
REACT_APP_DEBUG=true

# Feature Flags
REACT_APP_ENABLE_VOICE_RECORDING=true
REACT_APP_ENABLE_CHAT_SUPPORT=true
REACT_APP_ENABLE_ADMIN_PORTAL=true
REACT_APP_ENABLE_ANALYTICS=false

# Session Configuration
REACT_APP_SESSION_TIMEOUT=28800000
REACT_APP_OTP_EXPIRY_MINUTES=10

# File Upload Configuration
REACT_APP_MAX_FILE_SIZE=50
REACT_APP_ALLOWED_AUDIO_FORMATS=wav,mp3,m4a,ogg

# UI Configuration
REACT_APP_THEME=healthcare
REACT_APP_PRIMARY_COLOR=#1f4e79
REACT_APP_SECONDARY_COLOR=#2c5aa0

# External Services (when needed)
# REACT_APP_AZURE_STORAGE_URL=https://your-storage.blob.core.windows.net
# REACT_APP_ANALYTICS_ID=your-analytics-id

# Development Configuration
GENERATE_SOURCEMAP=true
DISABLE_ESLINT_PLUGIN=false
FAST_REFRESH=true

# Build Configuration
BUILD_PATH=build
PUBLIC_URL=/

# Browser Support
BROWSERSLIST_ENV=development